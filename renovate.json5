{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended"
  ],
  "enabledManagers": [
    "mix"
  ],
  packageRules: [
    {
      "groupName": "mix",
      "matchManagers": [
        "mix"
      ],
    },
    {
      "matchDepNames": ["git_lib_pkg"],
      "versioning": "semver"
    },
    {
      "matchDepNames": ["oban_pro"],
      "registryUrl": "https://getoban.pro/repo"
    }
  ],
}
